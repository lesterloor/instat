include pricenav.pug
.demo-btns
	header
		h2 Social Media Promotion 
	.info
		.buttons
			p
				a.modal__trigger(href="", data-modal="#modal") Followers: #{iFunnymemeInfo.followers.toLocaleString()}
				a.modal__trigger(href="", data-modal="#modal2") Followers: #{FirstscenesInfo.followers.toLocaleString()}
				a.modal__trigger(href="", data-modal="#modal3") Followers: #{Waltdisney1901Info.followers.toLocaleString()} 
				a.modal__trigger(href="", data-modal="#modal4") Followers: #{videostylistInfo.followers.toLocaleString()} 
				a.modal__trigger(href="", data-modal="#modal5") Followers: #{lesterInfo.followers.toLocaleString()}
				a.modal__trigger(href="", data-modal="#modal6" id="total") Followers: #{iFunnymemeInfo.followers + FirstscenesInfo.followers + Waltdisney1901Info.followers+ videostylistInfo.followers+ lesterInfo.followers}
				
		p
			| Purchases over $100 include FREE 24 hour story post with swipe up
		p
			| Kik : Super or insta@theblumarket.com
// Modal
#modal.modal.modal__bg(role="dialog", aria-hidden="true")
	.modal__dialog
		.modal__content
			h1 @iFunnymeme
			ul(class="Pricelist")
				li 1 hour: $60
				li 2 hours: $110
				li 3 hours: $150
				li 12 hours: $175
				li 24 hours: $200
				li Gender: 48% M | 62% F
				li Location: NY,UK,Canada
				li Active Time: 12pm-9pm EST
			
			// modal close button
			a.modal__close.demo-close(href="")
				svg(viewbox="0 0 24 24")
					path(d="M19 6.41l-1.41-1.41-5.59 5.59-5.59-5.59-1.41 1.41 5.59 5.59-5.59 5.59 1.41 1.41 5.59-5.59 5.59 5.59 1.41-1.41-5.59-5.59z")
